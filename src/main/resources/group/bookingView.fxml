<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group.frontend.bookingController">
   <children>
      <Label layoutY="-1.0" text="FlyDreamAir Customer Management System">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </Label>
      <Label layoutX="80.0" layoutY="88.0" text="Manage Booking">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="changeSeatButton" layoutX="514.0" layoutY="162.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="205.0" style="-fx-background-color: #1f8bff;" text="Change Seat" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="logoutButton" layoutX="620.0" mnemonicParsing="false" onAction="#logout" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #114680; -fx-background-radius: 0;" text="Logout" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="710.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #ff5656; -fx-background-radius: 0;" text="Exit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="updateClassButton" layoutX="514.0" layoutY="215.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="205.0" style="-fx-background-color: #1f8bff;" text="Update Class" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="updateServicesButton" layoutX="514.0" layoutY="273.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="205.0" style="-fx-background-color: #1f8bff;" text="Update Services" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelBookingButton" layoutX="514.0" layoutY="331.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="205.0" style="-fx-background-color: #1f8bff;" text="Cancel Booking" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutY="560.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #114680; -fx-background-radius: 0;" text="Back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Label layoutX="80.0" layoutY="144.0" text="ideekd">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
